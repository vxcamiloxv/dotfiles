#!/usr/bin/env xdg-open
[Desktop Entry]
Name=Iron Web Browser
Comment=Browse the World Wide Web
Icon=iron
Exec=/opt/iron/iron --disable-gpu-watchdog --ppapi-flash-path=/opt/iron/lib/libpepflashplayer.so %U
Terminal=false
Type=Application
Categories=Network;
StartupWMClass=Iron
MimeType=text/html;text/xml;application/xhtml+xml;text/mml;x-scheme-handler/http;x-scheme-handler/https;
